{
	"name": "ts-talk",
	"description": "StackOverflow code run buttons",
	"version": "0.0.1",
	"manifest_version": 2,
	"permissions": [
		"tabs", "background"
	],
	"web_accessible_resources": [
		"app/addOpenButtons.js",
		"app/addOpenButtons.map.js",
		"app/utils.js",
		"app/utils.map.js"
	],
	"icons": {
		"128": "resources/icon128.png"
	},
	"content_scripts": [{
        "matches": [
            "https://*.stackoverflow.com/*",
            "http://*.stackoverflow.com/*"
        ],
        "js": ["contentScript.js"]
    }]
}