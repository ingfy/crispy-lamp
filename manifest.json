{
	"manifest_version": 2,
	"permissions": [
		"tabs", "background"
	],
	"icons": {
		"128": "resources/icon128.png"
	},
	"background": {
		"persistent": false
	},
	"content_scripts": [{
        "matches": [
            "https://*.stackoverflow.com/*",
            "http://*.stackoverflow.com/*"
        ],
        "js": ["contentScript.js"]
    }]
}